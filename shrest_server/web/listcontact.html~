<script>
function displayVals() {
  var multipleValues = $( "#from_id" ).val() || [];
  
  if(multipleValues == "0") window.alert(multipleValues);
  else{
	$.getJSON("listcontact/contact_content", {from_id: multipleValues},
		function(jsonData) {
		  alert(jsonData);
	});
  }
}

 
$( "#from_id" ).change( displayVals );
//$( "select" ).change( displayVals );
displayVals();
</script>
 

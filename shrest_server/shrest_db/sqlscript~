

	pstmt->setInt(1, id_);
	pstmt->setString(2, firstName_);
	pstmt->setString(3, lastName_);
	pstmt->setInt(4, age_);
	pstmt->setString(5, phone_);
	pstmt->setString(6, address_);


CREATE TABLE customer('id_' int, 'firstName' char(20), 'lastName' char(20), 'age' int, 'phone' char(20), 'address' varchar(100));

INSERT TABLE customer(id_, firstName, lastName, age, phone, address) VALUES(1, "SHUHAI", "CUI", 50, "6666666666", "van ash");
